<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.servicecorp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cunyservicehours"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Welcome to ServiceCorp App" />
        <activity
            android:name=".WeekOne"
            android:label="List"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".help"
            android:label="Help"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".navigation"
            android:label="@string/title_activity_navigation"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.servicecorp.MainActivity" />
        </activity>
        <activity android:name=".Camera"></activity>
    </application>

</manifest>